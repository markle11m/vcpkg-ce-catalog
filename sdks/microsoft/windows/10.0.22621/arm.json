{
  "id": "sdks/microsoft/windows/arm",
  "version": "10.0.22621",
  "summary": "Microsoft Windows SDK (targeting ARM)",
  "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting ARM).",
  "options": [
    "dependencyOnly"
  ],
  "contacts": {
    "Mark Levine": {
      "email": "markle@microsoft.com",
      "role": [
        "publisher"
      ]
    }
  },
  "requires": {
    "sdks/microsoft/windows/core": "10.0.22621"
  },
  "install": [
    {
      "unzip": [
        "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.arm/10.0.22621.1"
      ],
      "sha256": "C40708888461F9A626F12745B0A23D4D4BA9192BB77D0B41B3062A8E3146EEE5"
    }
  ],
  "exports": {
    "paths": {
      "LIB": [
        "./c/ucrt/arm",
        "./c/um/arm"
      ],
      "PATH": "./bin"
    },
    "msbuild-properties": {
      "WindowsSDK_LibraryPath_arm": "{root}\\c\\um\\arm",
      "UniversalCRT_LibraryPath_arm": "{root}\\c\\ucrt\\arm"
    }
  }
}