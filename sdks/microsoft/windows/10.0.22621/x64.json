{
  "id": "sdks/microsoft/windows/x64",
  "version": "10.0.22621",
  "summary": "Microsoft Windows SDK (targeting x64)",
  "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting x64).",
  "options": [
    "dependencyOnly"
  ],
  "contacts": {
    "Mark Levine": {
      "email": "markle@microsoft.com",
      "role": [
        "publisher"
      ]
    }
  },
  "requires": {
    "sdks/microsoft/windows/core": "10.0.22621"
  },
  "install": [
    {
      "unzip": [
        "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.x64/10.0.22621.1"
      ],
      "sha256": "6C4C3EFB8A62A10D0EA8627A5FB247E9CDD8E4119EE823468A51F3D599802FF8"
    }
  ],
  "exports": {
    "paths": {
      "LIB": [
        "./c/ucrt/x64",
        "./c/um/x64"
      ],
      "PATH": "./bin"
    },
    "msbuild-properties": {
      "WindowsSDK_LibraryPath_x64": "{root}\\c\\um\\x64",
      "UniversalCRT_LibraryPath_x64": "{root}\\c\\ucrt\\x64"
    }
  }
}