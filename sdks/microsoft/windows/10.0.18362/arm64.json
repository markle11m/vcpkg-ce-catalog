{
  "id": "sdks/microsoft/windows/windows.arm64",
  "version": "10.0.18362",
  "summary": "Microsoft Windows SDK (targeting ARM64)",
  "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting ARM64).",
  "options": [
    "dependencyOnly"
  ],
  "contacts": {
    "Mark Levine": {
      "email": "markle@microsoft.com",
      "role": [
        "publisher"
      ]
    }
  },
  "requires": {
    "sdks/microsoft/windows/core": "10.0.18362"
  },
  "install": [
    {
      "unzip": [
        "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.arm64/10.0.18362.5"
      ],
      "sha256": "41F33754CE5A17E7EE5846414968ACA394D701B3D9F556C9A9F3F108E11DF4E1"
    }
  ],
  "exports": {
    "paths": {
      "LIB": [
        "./c/ucrt/arm64",
        "./c/um/arm64"
      ],
      "PATH": "./bin"
    },
    "msbuild-properties": {
      "WindowsSDK_LibraryPath_arm64": "{root}\\c\\um\\arm64",
      "UniversalCRT_LibraryPath_arm64": "{root}\\c\\ucrt\\arm64"
    }
  }
}