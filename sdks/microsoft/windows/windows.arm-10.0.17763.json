  "install": [
    {
      "unzip": [
        "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.arm/10.0.17763.4"
      ],
      "sha256": "7B32310774EBA270ED77D3BDD5081A036918953A524B99EE421CA404EEFD5135"
    }
  ],
  "exports": {
    "paths": {
      "LIB": [
        "./c/ucrt/arm",
        "./c/um/arm"
      ],
      "PATH": "./bin"
    },
    "msbuild-properties": {
      "WindowsSDK_LibraryPath_arm": "{root}\\c\\um\\arm",
      "UniversalCRT_LibraryPath_arm": "{root}\\c\\ucrt\\arm"
    }
  },
  "id": "sdks/microsoft/windows/windows.arm",
  "version": "10.0.17763",
  "summary": "Microsoft Windows SDK (targeting ARM)",
  "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting ARM).",
  "options": [
    "dependencyOnly"
  ]
}