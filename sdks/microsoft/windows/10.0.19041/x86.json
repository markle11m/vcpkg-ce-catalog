{
  "id": "sdks/microsoft/windows/windows.x86",
  "version": "10.0.19041",
  "summary": "Microsoft Windows SDK (targeting x86)",
  "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting x86).",
  "options": [
    "dependencyOnly"
  ],
  "contacts": {
    "Mark Levine": {
      "email": "markle@microsoft.com",
      "role": [
        "publisher"
      ]
    }
  },
  "requires": {
    "sdks/microsoft/windows/core": "10.0.19041"
  },
  "install": [
    {
      "unzip": [
        "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.x86/10.0.19041.5"
      ],
      "sha256": "7D7213F0A675877437C9B1CA4AB4FA8CBFF9F999A3F0121D2763C17A9B26BB42"
    }
  ],
  "exports": {
    "paths": {
      "LIB": [
        "./c/ucrt/x86",
        "./c/um/x86"
      ],
      "PATH": "./bin"
    },
    "msbuild-properties": {
      "WindowsSDK_LibraryPath_x86": "{root}\\c\\um\\x86",
      "UniversalCRT_LibraryPath_x86": "{root}\\c\\ucrt\\x86"
    }
  }
}