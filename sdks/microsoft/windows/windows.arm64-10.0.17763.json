{
    "info": {
        "id": "sdks/microsoft/windows/windows.arm64",
        "version": "10.0.17763",
        "description": "The Windows SDK available as a NuGet package for more seamless acquisition and CI/CD integration. This package is designed for C++ applications (targeting ARM64).",
        "summary": "Microsoft Windows SDK (targeting ARM64)",
        "options": [ "dependencyOnly" ]
    },
    "contacts": {
        "Mark Levine": {
            "email": "markle@microsoft.com",
            "role": [ "publisher" ]
        }
    },
    "install": [
        {
            "unzip": [
                "https://www.nuget.org/api/v2/package/Microsoft.Windows.SDK.cpp.arm64/10.0.17763.4"
            ],
            "sha256": "6013923CF3ECEB61B5637E292AF1146A4816859EE3193C832D3CDDA55FE3B0D9"
        }
    ],
    "exports": {
        "paths": {
            "LIB": [
                "./c/ucrt/arm64",
                "./c/um/arm64"
            ],
            "PATH": "./bin"
        },
        "locations": {
            "WindowsSDK_LibraryPath_arm64": "./c/um/arm64",
            "UniversalCRT_LibraryPath_arm64": "./c/ucrt/arm64"
        }
    }
}
